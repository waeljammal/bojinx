<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%" 
		 xmlns:test="com.bojinx.test.*">
	
	<fx:Script>
		<![CDATA[
			import com.bojinx.message.TestMessageOne;
			public var wiredSubModule:String = "yes";
			
			[Bindable]
			[Inject]
			public var test:TestObjectOne;
			
			[Message(name="filterOne", priority="100")]
			public function handleFilterOne(m:TestMessageOne):void
			{
				output.text += "\nReceived TestMessage in handleFilterOne [Index]: " + m.index + " [Priority]: 100"; 
				m.index++;
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:Rect percentHeight="100" percentWidth="100">
		<s:fill>
			<s:SolidColor color="0X257312" />
		</s:fill> 
	</s:Rect>
	
	<s:Label top="15" horizontalCenter="0" text="Test Component Two" />
	<s:TextInput top="10" right="10" text="Injected {test.value}" editable="false" width="150" />
	
	<s:VGroup top="40" left="10" right="10" bottom="10">
		<s:TextArea width="100%" height="50" id="output" text="Test Component Two Output" />
	</s:VGroup>
	
</s:Group>
